.forumCenter-container {
width: 105vw;
background-color: #f7f6fa;
}

.title {
padding: 25rpx 0 0 0;
}

button {
color: #fff;
background-color: #67c23a;
border-color: #67c23a;
font-size: 40rpx;
}





.content {
letter-spacing: 2rpx;
display: flex;
flex-direction: column;
height: 100vh;
margin: 0 20rpx;
}

.list {
height: inherit;
width: inherit;
display: flex;
flex-direction: column;
}

.nodata {
text-align: center;
padding: 20rpx;
color: rgb(73, 54, 54);
}

.list-item {
width: 100%;
display: flex;
justify-content: space-between;
background: rgb(232, 235, 235);
border-radius: 28rpx;
box-shadow: 0 0 10rpx rgba(0, 0, 0, .3);
height: 40vh;
margin: 20rpx 0;
}



.right {
width: 100%;
padding: 10px;
}

.line {
color: #f31010;
font-size: 14px;
display: flex;
justify-content: space-between;
width: 100%;
}

.username,
.publishdate {
font-size: 24rpx;
color: #bbb;
}



@import "/static/iconfig/iconfont.wxss";

.search {
height: 10vh;
background-color: white;
display: flex;
align-items: center;
padding: 0 20rpx;
font-size: 24rpx;
position: relative;
}



.search-input {
background-color: #f5f5f5;
height: 6vh;
color: #333;
display: -webkit-box;
display: -webkit-flex;
width: 70vw;
border-radius: 50rpx;
padding-left: 30px;
position: relative;
}

.icons {
width: 6vw;
position: absolute;
left: 20rpx;
line-height: 6vh;
font-size: 30rpx;
}

.searchBtn {
background-color: #f5f5f5;
margin: 20rpx;
height: 6vh;
width: 16vw;
line-height: 6vh;
text-align: center;
letter-spacing: 2rpx;
border-radius: 50rpx;
}